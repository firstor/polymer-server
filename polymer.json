{
  "entrypoint": "index.html",
  "shell": "src/main.html",
  "fragments": [
    "src/e404.html",
    "src/views/icon-toggle-view.html",
    "src/views/hirst-dialog-view.html",
    "src/views/paper-item-view.html",
    "src/views/paper-search-view.html",
    "src/views/paper-chip-view.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "name": "es5",
      "preset": "es5-bundled"
    },
    {
      "name": "bundled",
      "preset": "es6-bundled"
    },
    {
      "name": "unbundled",
      "preset": "es6-unbundled"
    }
  ]
}
